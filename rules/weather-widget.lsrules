{
  "name": "weather-widget",
  "description": "Rules for the MacOS weather widget.",
  "rules": [
    {
      "action": "allow",
      "process": "/System/Applications/Weather.app/Contents/PlugIns/WeatherWidget.appex/Contents/MacOS/WeatherWidget",
      "remote-domains": [
        "apple.news"
      ],
      "ports": "443",
      "protocol": "tcp"
    }
  ]
}
