{
  "name": "Arq",
  "description": "Rules for Arq - Cloud backup software for Mac.",
  "rules": [
    {
      "action": "allow",
      "process": "/Applications/Arq.app/Contents/Resources/ArqAgent.app/Contents/MacOS/ArqAgent",
      "remote-domains": [
        "1drv.com",
        "arqbackup.com",
        "backblaze.com",
        "backblazeb2.com",
        "microsoft.com",
        "microsoftonline.com"
      ],
      "ports": "443",
      "protocol": "tcp"
    },
    {
      "action": "allow",
      "process": "/Applications/Arq.app/Contents/Resources/ArqAgent.app/Contents/MacOS/ArqAgent",
      "remote-domains": [
        "github.com"
      ],
      "ports": "443",
      "protocol": "tcp"
    },
    {
      "action": "allow",
      "process": "/Applications/Arq.app/Contents/Resources/ArqAgent.app/Contents/MacOS/ArqAgent",
      "remote-domains": [
        "github.com"
      ],
      "ports": "443",
      "protocol": "tcp",
      "via": "/usr/local/Cellar/git-lfs/3.3.0/bin/git-lfs"
    },
    {
      "action": "allow",
      "process": "/Applications/Arq.app/Contents/Resources/ArqAgent.app/Contents/MacOS/ArqAgent",
      "remote-domains": [
        "github.com"
      ],
      "ports": "443",
      "protocol": "tcp",
      "via": "/usr/local/Cellar/git/2.39.2/libexec/git-core/git-remote-http"
    },
    {
      "action": "allow",
      "process": "/Applications/Arq.app/Contents/Resources/ArqAgent.app/Contents/MacOS/ArqAgent",
      "remote-domains": [
        "dropboxapi.com"
      ],
      "ports": "443",
      "protocol": "tcp"
    }
  ]
}
